<?xml version="1.0"?>
<dialog title="Authentication">
	<vbox margin="2">
		<caption bind="left right" id="instruction" text="Please enter the requested authentication information" />
		<hbox bind="left right">
			<caption width="50" id="label-1" text="Password:"/>
			<edittext width="120" bind="left right" id="edit-1"/>
		</hbox>
		<hbox bind="left right">
			<caption width="50" id="label-2" text="Password:"/>
			<edittext width="120" bind="left right" id="edit-2"/>
		</hbox>
		<hbox bind="left right">
			<caption width="50" id="label-3" text="Password:"/>
			<edittext width="120" bind="left right" id="edit-3"/>
		</hbox>
		<hbox bind="left right">
			<caption width="50" id="label-4" text="Password:"/>
			<edittext width="120" bind="left right" id="edit-4"/>
		</hbox>
		<hbox bind="left right">
			<caption width="50" id="label-5" text="Password:"/>
			<edittext width="120" bind="left right" id="edit-5"/>
